<aura:component>
    <aura:registerEvent name="ExamsLandingEvent" type="c:ExamsLandingEvent"/>
    <aura:attribute name="activeTitan" type="Titan__c" default=""/>
    <div>
        <div>
            <!-- Contact information Component, allows the user to view their contact information -->
            <span> <div class="container slds-m-around_large" id="contactinfo">
                <lightning:card title="{!v.activeTitan.Name + ' Exams'}" iconName="standard:contact">
                    <div class="slds-grid">
                        <ul>    
                            <li class=" slds-text-heading_medium slds-col slds-size_12-of-12"> Exam #1 {!v.activeTitan.Name}</li><br/>
                            <li class=" slds-text-heading_medium slds-col slds-size_12-of-12"> Exam #2 {!v.activeTitan.Name}</li><br/>
                            <li class=" slds-text-heading_medium slds-col slds-size_12-of-12"> Exam #3 {!v.activeTitan.Name}</li><br/>
                            <li class=" slds-text-heading_medium slds-col slds-size_12-of-12"> Exam #4 {!v.activeTitan.Name}</li><br/>
                        </ul>
                    </div>
                </lightning:card>
                </div></span>
        </div>
    </div>
</aura:component>