<aura:component controller="CohortButtonsAuraController">

    <aura:registerEvent name="UpdateCohortEvent" type="c:UpdateCohortEvent"/>

    <aura:attribute name="ActiveCohorts" type="Cohort__c[]"/>


<aura:iteration items="{!v.ActiveCohorts}" var="Cohort">
    <lightning:button value="{!Cohort}" onclick="{!c.SelectCohort}">{!Cohort.Name}</lightning:button>
</aura:iteration>


</aura:component>	
